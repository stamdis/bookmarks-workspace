{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "bookmarks",
  "projects": {
    "bookmarks": {
      "projectType": "application",
      "root": "apps/bookmarks",
      "sourceRoot": "apps/bookmarks/src",
      "prefix": "bookmarks-workspace",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/bookmarks",
            "index": "apps/bookmarks/src/index.html",
            "main": "apps/bookmarks/src/main.ts",
            "polyfills": "apps/bookmarks/src/polyfills.ts",
            "tsConfig": "apps/bookmarks/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/bookmarks/src/favicon.ico",
              "apps/bookmarks/src/assets"
            ],
            "styles": ["./node_modules/@angular/material/prebuilt-themes/purple-green.css", "apps/bookmarks/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/bookmarks/src/environments/environment.ts",
                  "with": "apps/bookmarks/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "bookmarks:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "bookmarks:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "bookmarks:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/bookmarks/src/**/*.ts",
              "apps/bookmarks/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/bookmarks"],
          "options": {
            "jestConfig": "apps/bookmarks/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "bookmarks-e2e": {
      "root": "apps/bookmarks-e2e",
      "sourceRoot": "apps/bookmarks-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/bookmarks-e2e/cypress.json",
            "tsConfig": "apps/bookmarks-e2e/tsconfig.e2e.json",
            "devServerTarget": "bookmarks:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "bookmarks:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/bookmarks-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "angular-material": {
      "projectType": "library",
      "root": "libs/angular-material",
      "sourceRoot": "libs/angular-material/src",
      "prefix": "bookmarks-workspace",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/angular-material/src/**/*.ts",
              "libs/angular-material/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/angular-material"],
          "options": {
            "jestConfig": "libs/angular-material/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "bookmarks-lib": {
      "projectType": "library",
      "root": "libs/bookmarks-lib",
      "sourceRoot": "libs/bookmarks-lib/src",
      "prefix": "bookmarks-workspace",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/bookmarks-lib/src/**/*.ts",
              "libs/bookmarks-lib/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/bookmarks-lib"],
          "options": {
            "jestConfig": "libs/bookmarks-lib/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
